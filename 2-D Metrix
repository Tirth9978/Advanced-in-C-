(1) Transpose of a metrix
  --> Code :
      #include<iostream>
      using namespace std;
      
      int main(){
          int n;
          int m;
          cout << "n = ";
          cin >> n;
          cout << "m = ";
          cin >> m;
          int arr[n][m];
          for (int i=0;i<n;i++){
              for (int j=0;j<m;j++){
                  cout << "Entre the number:";
                  cin >> arr[i][j];
              }
          }
          for (int i=0;i<n;i++){
              for (int j= 0;j<m;j++){
                  cout << arr[i][j] << "  ";
              }
              cout << endl;
          }
          if ( m = n ){
              for (int i=0;i<n;i++){
                  for (int j=i;j<m;j++){                        <----(Hear int j = i is main step)
                      
                      int a= 0;
                      a = arr[i][j];
                      arr[i][j] = arr[j][i];
                      arr[j][i] = a;
                      
                  }
              }
              // cout 
              cout << "Transpose of a metrix: " << endl;
              for (int i=0;i<n;i++){
                  for (int j= 0;j<m;j++){
                      cout << arr[i][j] << "  ";
                  }
                  cout << endl;
              }
          }
          else {
              cout << "M and n should be same ";    
          }
          return 0;
      }
  --> Output :   
      1  2            1  3
              ===>  
      2  4            3  4 
**********************************************************************************************************************************************************************


(2)Multiplication of a metrix 
  --> Method 1:
    --Code :
      #include<iostream>
      using namespace std;
      
      int main(){
          int n1,n2,n3;
          cin >> n1>>n2>>n3;
          int m1[n1][n2];
          int m2[n2][n3];
          cout << "For metrix 1 :"<< endl;
          for (int i=0;i<n1;i++){
              for (int j=0;j<n2;j++){
                  cout << "Entre the number:";
                  cin >> m1[i][j];
              }
          }
          cout << "For metrix 2:" << endl;
          for (int i=0;i<n2;i++){
              for (int j=0;j<n3;j++){
                  cout << "Entre the number:";
                  cin >> m2[i][j];
              }
          }
          int ans[n1][n3];
          for (int i=0;i<n1;i++){
              for (int j=0;j<n3;j++){
                  ans[i][j] = 0;
              }
          }
          for (int i=0;i<n1;i++){
              for (int j=0;j<n3;j++){
                  for (int k=0;k<n2;k++){
                      ans[i][j] += m1[i][k] * m2[k][j];
                  }
              }
          }
          for (int i=0;i<n1;i++){
              for (int j=0;j<n3;j++){
                  cout << ans[i][j] << "  ";
              }
              cout << endl;
          }
          return 0;
      }

    --Result: 
      1    2              1    2                            7      10
                  X                        ==>    
      3    4              3    4                            15      22
